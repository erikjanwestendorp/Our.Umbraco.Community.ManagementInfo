{
    "$schema": "../../umbraco-package-schema.json",
    "name": "My workspace",
    "version": "0.1.0",
    "extensions": [
      {
        "type": "workspaceView",
        "alias": "DocumentTypeDefinition.WorkspaceView",
        "name": "Document Type Definition Workspace View",
        "element": "/App_Plugins/ManagementInfo/assets.js",
        "elementName": "my-element",
        "meta": {
          "label": "Definition",
          "pathname": "/my-workspace-view",
          "icon": "icon-code"
        },
        "conditions": [
          {
            "alias": "Umb.Condition.WorkspaceAlias",
            "match": "Umb.Workspace.DocumentType"
          }
        ]
      },
      {
        "type": "workspaceView",
        "alias": "MediaTypeDefinition.WorkspaceView",
        "name": "Media Type Definition Workspace View",
        "element": "/App_Plugins/ManagementInfo/assets.js",
        "elementName": "my-element",
        "meta": {
          "label": "Definition",
          "pathname": "/my-workspace-view",
          "icon": "icon-code"
        },
        "conditions": [
          {
            "alias": "Umb.Condition.WorkspaceAlias",
            "match": "Umb.Workspace.MediaType"
          }
        ]
      },
      {
        "type": "workspaceView",
        "alias": "DataTypeDefinition.WorkspaceView",
        "name": "Data Type Definition Workspace View",
        "element": "/App_Plugins/ManagementInfo/assets.js",
        "elementName": "my-element",
        "meta": {
          "label": "Definition",
          "pathname": "/my-workspace-view",
          "icon": "icon-code"
        },
        "conditions": [
          {
            "alias": "Umb.Condition.WorkspaceAlias",
            "match": "Umb.Workspace.DataType"
          }
        ]
      },
      {
        "type": "workspaceView",
        "alias": "MemberTypeDefinition.WorkspaceView",
        "name": "Member Type Definition Workspace View",
        "element": "/App_Plugins/ManagementInfo/assets.js",
        "elementName": "my-element",
        "meta": {
          "label": "Definition",
          "pathname": "/my-workspace-view",
          "icon": "icon-code"
        },
        "conditions": [
          {
            "alias": "Umb.Condition.WorkspaceAlias",
            "match": "Umb.Workspace.MemberType"
          }
        ]
      }
    ]
}